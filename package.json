{
  "name": "compare-harga",
  "version": "1.0.0",
  "description": "Platform perbandingan harga terbaik untuk Shopee dan Lazada",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "nuxt dev",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "start": "node .output/server/index.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "nuxt typecheck",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf .nuxt .output dist node_modules/.cache",
    "clean:all": "rm -rf .nuxt .output dist node_modules package-lock.json && npm install",
    "db:seed": "node scripts/seed.js",
    "db:migrate": "node scripts/migrate.js",
    "task:check-alerts": "node server/tasks/checkPriceAlerts.js",
    "analyze": "nuxt build --analyze",
    "build:prod": "NODE_ENV=production nuxt build",
    "start:prod": "NODE_ENV=production node .output/server/index.mjs"
  },
  "dependencies": {
    "@headlessui/vue": "^1.7.23",
    "@heroicons/vue": "^2.2.0",
    "@nuxt/eslint": "^1.4.1",
    "@nuxt/fonts": "^0.11.4",
    "@nuxt/icon": "^1.14.0",
    "@nuxt/image": "^1.10.0",
    "@nuxt/scripts": "^0.11.8",
    "@nuxt/test-utils": "^3.19.1",
    "@nuxt/ui": "^3.2.0",
    "@pinia/nuxt": "^0.11.1",
    "@unhead/vue": "^2.0.11",
    "chart.js": "^4.5.0",
    "eslint": "^9.29.0",
    "mongodb": "^6.17.0",
    "mongoose": "^8.16.0",
    "nuxt": "^3.17.5",
    "pinia": "^3.0.3",
    "typescript": "^5.8.3",
    "vue": "^3.5.17",
    "vue-chartjs": "^5.3.2",
    "vue-router": "^4.5.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "price-comparison",
    "shopee",
    "lazada",
    "e-commerce",
    "malaysia",
    "nuxt",
    "vue"
  ],
  "author": "CompareHarga Team",
  "license": "MIT",
  "devDependencies": {
    "@nuxtjs/tailwindcss": "^6.14.0",
    "@sidebase/nuxt-auth": "^1.0.0",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  }
}
